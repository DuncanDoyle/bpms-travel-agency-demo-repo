      <bpmn2:script><![CDATA[System.out.println("[Prepare Data Script Task] Start of task... ");

/* Set initial default values for reviewRequired and reviewRequiredDetails variables*/
Boolean _reviewRequired = false;
String _reviewRequiredDetails = "Initial review";

/* Populating Applicant object */
org.specialtripsagency.Applicant _applicant = new org.specialtripsagency.Applicant();
_applicant.setName(applicantName);
_applicant.setEmailAddress(emailAddress);
_applicant.setNumberOfTravelers(numberOfTravelers);
_applicant.setOtherDetails(otherDetails);


/* Populating Flight object */
org.specialtripsagency.Flight _flight = new org.specialtripsagency.Flight();
_flight.setCompany("XYZ Airlines");
_flight.setPlaneId(111);
_flight.setPriceDiscount(0);
_flight.setRatePerPerson(525); /* CHANGEME */
_flight.setReturnDate(preferredDateOfArrival);
_flight.setTravelDate(preferredDateOfDeparture);
_flight.setStartCity(fromDestination);
_flight.setTargetCity(toDestination);

/* Populating Hotel object */
org.specialtripsagency.Hotel _hotel = new org.specialtripsagency.Hotel();
_hotel.setHotelId(999);
_hotel.setHotelName("XYZ Hotels");
_hotel.setLocation("Edinburgh");
_hotel.setDuration(5);
_hotel.setRoomPrice(80);
_hotel.setPriceDiscount(0);
_hotel.setRoomType("Standard Double bedroom");


/* Populating TravelDetails object */
org.specialtripsagency.TravelDetails _travelDetails = new org.specialtripsagency.TravelDetails();
_travelDetails.setFromDestination(fromDestination);
_travelDetails.setToDestination(toDestination);
_travelDetails.setPreferredDateOfDeparture(preferredDateOfDeparture);
_travelDetails.setPreferredDateOfArrival(preferredDateOfArrival);

/* Populating BookingObject object */
org.specialtripsagency.BookingObject _bookingObject = new org.specialtripsagency.BookingObject();
_bookingObject.setBookingId(12345);
_bookingObject.setApplicant(_applicant);
_bookingObject.setAvailableFlights(_flight);
bookingObject.setAvailableHotels(_hotel);
_bookingObject.setTravelDetails(_travelDetails);
_bookingObject.setTotalPrice(0); /* Setting initial total price value to avoid NullPointer Exception*/
_bookingObject.setAdditionalPriceDiscount(0);


/* set a process variable */
kcontext.setVariable("bookingObject", _bookingObject);
kcontext.setVariable("applicant", _applicant);
kcontext.setVariable("flight", _flight);
kcontext.setVariable("hotel", _hotel);
kcontext.setVariable("travelDetails", _travelDetails);
kcontext.setVariable("reviewRequired", _reviewRequired);
kcontext.setVariable("reviewRequiredDetails", _reviewRequiredDetails);



/* insert objects into working memory */
kcontext.getKnowledgeRuntime().insert(_bookingObject);
kcontext.getKnowledgeRuntime().insert(_applicant);
kcontext.getKnowledgeRuntime().insert(_flight);
kcontext.getKnowledgeRuntime().insert(_hotel);
kcontext.getKnowledgeRuntime().insert(_travelDetails);


System.out.println("[Prepare Data Script Task] End of task... ");]]></bpmn2:script>
