    <bpmn2:scriptTask id="_E22E7548-FA79-475F-9F90-94197FBDC2F4" drools:selectable="true" name="Prepare Data Script" scriptFormat="http://www.java.com/java">
      <bpmn2:extensionElements>
        <drools:onEntry-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("[Prepare Data Script - Before] Before Prepare Data Script");]]></drools:script>
        </drools:onEntry-script>
        <drools:onExit-script scriptFormat="http://www.java.com/java">
          <drools:script><![CDATA[System.out.println("[Prepare Data Script - After] After Prepare Data Script");]]></drools:script>
        </drools:onExit-script>
      </bpmn2:extensionElements>
      <bpmn2:incoming>_9A8F1D95-A872-4161-B457-7EC1ACF06555</bpmn2:incoming>
